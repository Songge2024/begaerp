		      Or solve like '${solve}%'  <!-- 备注 -->
		</if>
	    <if test="flag != null and flag != ''">
		      Or flag like '${flag}%'  <!-- 是否有效 -->
		</if>
	    <if test="type != null and type != ''">
		      Or type like '${type}%'  <!-- 周报类型 -->
		</if>
	    <if test="task_plan_num != null and task_plan_num != ''">
		      Or task_plan_num like '${task_plan_num}%'  <!-- 任务数量 -->
		</if>
	    <if test="task_finish_num != null and task_finish_num != ''">
		      Or task_finish_num like '${task_finish_num}%'  <!-- 完成数量 -->
		</if>
	</sql>
	
</mapper>