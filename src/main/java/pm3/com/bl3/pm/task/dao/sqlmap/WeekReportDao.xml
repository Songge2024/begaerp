	    <if test="update_time != null and update_time != ''">
		      AND update_time like '${update_time}%'  <!-- 更新时间 -->
		</if>
	    <if test="create_time != null and create_time != ''">
		      AND create_time like '${create_time}%'  <!-- 创建时间 -->
		</if>
	    <if test="solve != null and solve != ''">
		      AND solve like '${solve}%'  <!-- 目前情况说明 -->
		</if>
		<if test="description != null and description != ''">
		      AND description like '${description}%'  <!-- 对外情况说明 -->
		</if>
	</sql>
	<!-- Where模糊匹配字段 并集 -->
	<sql id="likeOr">
		
	    <if test="test_code != null and test_code != ''">
		      Or test_code like '${test_code}%'  <!-- 周报编码 -->
		</if>
	    <if test="remarks != null and remarks != ''">
		      Or remarks like '${remarks}%'  <!-- 任务描述 -->
		</if>
	 
	    <if test="finish_time != null and finish_time != ''">
		      Or finish_time like '${finish_time}%'  <!-- 实际完成实际 -->
		</if>
	    <if test="finish_cond != null and finish_cond != ''">
		      Or finish_cond like '${finish_cond}%'  <!-- 完成情况 -->
		</if>
	    <if test="descc != null and descc != ''">
		      Or descc like '${descc}%'  <!-- 任务偏差分析及解决措施 -->
		</if>
	   
	    <if test="week_name != null and week_name != ''">
		      Or week_name like '${week_name}%'  <!-- 周报名称 -->
		</if>
	    <if test="state != null and state != ''">
		      Or state like '${state}%'  <!-- 周报状态 -->
		</if>
	    <if test="update_time != null and update_time != ''">
		      Or update_time like '${update_time}%'  <!-- 更新时间 -->
		</if>
	    <if test="create_time != null and create_time != ''">
		      Or create_time like '${create_time}%'  <!-- 创建时间 -->
		</if>
	     <if test="solve != null and solve != ''">
		      Or solve like '${solve}%'  <!-- 目前情况说明 -->
		</if>
		<if test="description != null and description != ''">
		      Or description like '${description}%'  <!-- 对外情况说明 -->
		</if>
	</sql>
	<!-- 查询项目数据 -->
	<select id="getProjt" resultType="Dto">
		SELECT
        	q.*
        	,t.*	 
        FROM ta_week_report q,ta_week t
        WHERE q.test_code=t.test_code AND t.proj_code= #{proj_code}
	</select>
	
	<update id="copy" parameterType="WeekReportPO">
		UPDATE ta_week
		<set>
			<if test="solve != null">
		         description = #{solve, jdbcType=VARCHAR},  <!-- 责任人 -->
			</if>
			</set>
		WHERE  test_code = #{test_code}
	</update>
	
</mapper>