<%@ page contentType="text/html; charset=utf-8"%>
<%@ include file="/WEB-INF/jsp/common/tags.jsp"%>
<aos:window 
	id="${tableDto.lowclassname}_create_win" 
	title="新增${tableDto.comment}"
>
	<aos:formpanel 
		id="${tableDto.lowclassname}_create_form"
	 	width="450"
	  	layout="anchor" 
	  	labelWidth="70"
	>
	</aos:formpanel>
	<aos:docked dock="bottom" ui="footer">
		<aos:dockeditem xtype="tbfill" />
		<aos:dockeditem onclick="${tableDto.lowclassname}_create" text="保存" icon="ok.png" />
		<aos:dockeditem onclick="#${tableDto.lowclassname}_create_win.hide();" text="关闭" icon="close.png" />
	</aos:docked>
</aos:window>